
  <p><strong>On Lists:</strong>
    <% movie.lists.by_user(current_user).each do |list| %>
      <span class="tag-link"><%= link_to "#{list.name}", user_list_path(current_user, list.slug), id: "show_list_link_on_list_movies_partial" %></span><p><%= link_to "x", delete_listing_path(list.id, movie.id), id: "remove_movie_link_movies_partial", method: :delete, remote: true, data: { confirm: "Are you sure?" } %></p>
    <% end %> <!-- # list loop -->
  </p>
