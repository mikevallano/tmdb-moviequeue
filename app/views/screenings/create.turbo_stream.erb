<%= turbo_stream.update(
      "screenings_#{@movie.tmdb_id}",
      partial: 'movies/screenings',
      locals: { movie: @movie }
    )
%>
<%= turbo_stream.update(
      "review_#{@movie.tmdb_id}",
      partial: 'movies/movie_review',
      locals: { movie: @movie }
    )
%>
<%= turbo_stream.update(
      "screening_#{@movie.tmdb_id}",
      partial: 'movies/movie_screening',
      locals: { movie: @movie }
    )
%>
<%= turbo_stream.update(
      "movie_rating_#{@movie.tmdb_id}",
      partial: 'movies/movie_rating',
      locals: { movie: @movie }
    )
%>
