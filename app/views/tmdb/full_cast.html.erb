<h1>Full Cast:</h1>

<h2>Actors:</h2>

<% @actors.each do |actor| %>
  <%= image_tag "http://image.tmdb.org/t/p/w185#{actor.profile_path}" if actor.profile_path.present? %><br>
  <p>Name: <%= link_to "#{actor.name}", actor_more_path(actor_id: actor.actor_id) %><p>
  <p>Character: <%= actor.character_name %><p>
  <hr>
<% end %>